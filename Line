class Line:
    
    def __init__(self,coor1,coor2):
        self.coor1 = coor1
        self.coor2 = coor2
    
    def distance(self):
        d = ((self.coor2[0]-self.coor1[0])**2 + (self.coor2[1]-self.coor1[1])**2)**(1/2)
        return d
    
    def slope(self):
        s = (self.coor2[1]-self.coor1[1]) / (self.coor2[0]-self.coor1[0])
        return s

c1 = (2,3)
c2 = (4,5)

l = Line(c1,c2)
print(l.distance)
print(l.slope)
    
